{
  "name": "land-tech",
  "version": "1.0.0",
  "description": "Corporate Land Ownership",
  "main": "./dist/index.js",
  "bin": {
    "land-tech": "./dist/index.js"
  },
  "scripts": {
    "start": "npm run build && babel-node ./dist/index.js",
    "test:tdd": "jest --watch",
    "test:no-coverage": "jest",
    "test:nc": "jest",
    "test:file": "jest --",
    "test:file:watch": "jest --watch --",
    "test": "jest --coverage",
    "build": "rimraf dist/ && babel src/ -d dist --ignore 'src/**/*.test.js'",
    "pretest": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix",
    "lint:eslint": "eslint . --ext .js,.jsx",
    "lint:eslint:fix": "eslint . --ext .js,.jsx --fix",
    "prettify": "prettier --write **/*.{js,jsx,json}",
    "lint-staged": "lint-staged",
    "lint": "eslint ."
  },
  "author": "rjgreaves@hotmail.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "commander": "^7.0.0",
    "yargs": "^16.2.0"
  }
}
